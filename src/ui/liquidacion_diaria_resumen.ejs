<link rel="stylesheet" href="style/sliderMenu.css" />
<link rel="stylesheet" href="style/style.css" />
<div
  id="elementoImprimir"
  style="background-color: #fff; padding: 20px; overflow-y: scroll"
>
  <div class="flexColumn" style="justify-content: center">
    <div class="flexRow" style="justify-content: center; margin-bottom: 20px">
      <h2>AGROINSUMOS MONGE</h2>
    </div>
    <div
      class="flexRow"
      style="justify-content: space-around; margin-bottom: 20px"
    >
      <h3>LIQUIDACION</h3>
      <h3 id="fechaActual"></h3>
    </div>
  </div>
  <div
    class="flexColumn"
    style="
      border: 2px solid #000;
      padding: 20px 100px;
      margin: 0px 5%;
      border-radius: 9px;
    "
  >
    <div
      class="flexColumn"
      style="border: 1px solid #000; padding: 10px 100px; margin-bottom: 40px"
    >
      <div style="text-align: center">
        <h4><strong> VENTAS DEL DIA</strong></h4>
      </div>
      <div
        class="flexRow"
        style="align-items: center; justify-content: space-between"
      >
        <p>VENTAS DE CONTADO</p>
        <div class="cajaInput">
          <div
            class="imagenInput"
            style="border: #000 1px solid; background-color: #fff"
          >
            <input
              style="width: 15vh; background-color: #fff"
              type="text"
              name="ventasContado"
              id="ventasContado"
              class="estiloInputLogin"
              tabindex="-1"
            />
          </div>
        </div>
      </div>
      <div
        class="flexRow"
        style="align-items: center; justify-content: space-between"
      >
        <p>VENTAS DE CREDITO</p>
        <div class="cajaInput">
          <div
            class="imagenInput"
            style="border: #000 1px solid; background-color: #fff"
