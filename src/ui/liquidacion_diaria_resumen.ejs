            <input
              style="width: 15vh; background-color: #fff"
              type="text"
              name="ventasDeposito"
              id="ventasDeposito"
              class="estiloInputLogin"
              tabindex="-1"
            />
          </div>
        </div>
      </div>
      <div
        class="flexRow"
        style="align-items: center; justify-content: space-evenly"
      >
        <p>TOTAL DE VENTAS</p>
        <div class="cajaInput">
          <div
            class="imagenInput"
            style="border: #000 1px solid; background-color: #fff"
          >
            <input
              style="width: 15vh; background-color: #fff"
              type="text"
              name="totalVentas"
              id="totalVentas"
              class="estiloInputLogin"
              tabindex="-1"
            />
          </div>
        </div>
      </div>
    </div>
    <div
      class="flexColumn"
      style="border: 1px solid #000; padding: 10px 100px; margin-bottom: 40px"
    >
      <div style="text-align: center">
        <h4><strong> RECUPERACIONES DE LAS VENTAS</strong></h4>
      </div>
