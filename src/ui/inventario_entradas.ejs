              type="text"
              name="entrada_otros_gastos"
              id="entrada_otros_gastos"
              class="estiloInputLogin"
              tabindex="3"
            />
          </div>
        </div>
        <div class="cajaInput">
          <label for="entrada_tipo_pago">Tipo de Pago</label>
          <div class="imagenInput">
            <img src="icons/pencil.png" alt="" />
            <select
              class="estiloInputLogin"
              name="entrada_tipo_pago"
              id="entrada_tipo_pago"
              style="width: 13vh"
              tabindex="4"
            >
              <option value="">Seleccione</option>
              <option value="">Efectivo</option>
              <option value="">Credito</option>
            </select>
          </div>
        </div>
      </div>
    </div>
    <div class="botonesDashboard">
      <div class="flexColumn space-around">
        <div class="flexColumn">
          <button
            tabindex="-1"
            id="btnNuevoDerivado"
            class="boton colorSecundario"
          >
            NUEVO PRODUCTO
          </button>
          <button
            tabindex="-1"
            id="btnNuevoDerivado"
            class="boton colorSecundario"
          >
            NUEVO DERIVADO
          </button>
          <button tabindex="-1" id="btnNuevoLote" class="boton colorPrimario">
            NUEVO LOTE
          </button>
        </div>
        <div class="flexColumn">
          <button tabindex="5" id="btnAceptar" class="boton colorSecundario">
            ACEPTAR
          </button>
          <button tabindex="6" id="btnLimpiar" class="boton colorAmarillo">
            LIMPIAR
          </button>
        </div>
      </div>
    </div>
