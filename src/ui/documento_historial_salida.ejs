<%- include('./partials/header.ejs') %>
<link rel="stylesheet" href="style/style.css" />
<div
  class="contenedorDashboard"
  id="elementoImprimir"
  style="background-color: #fff; height: 80vh"
>
  <div
    class="flexRow"
    style="align-items: center; justify-content: space-evenly"
  >
    <img
      src="icons/empresaIcono.png"
      alt=""
      style="width: 150px; margin-bottom: 30px"
    />
    <div class="flexColumn" style="text-align: center">
      <h3 style="margin-bottom: 10px">
        AGROINSUMOS MONGE | SALIDA DE PRODUCTO
      </h3>
      <div style="justify-items: center" class="flexRow">
        <p style="margin-right: 10px"><strong>FECHA DE EMISION: </strong></p>
        <p id="fechaActual" style="margin-bottom: 10px"></p>
      </div>
      <div style="justify-items: center" class="flexRow">
        <p style="margin-right: 10px"><strong>NUMERO DE SERIE:</strong></p>
        <p id="numeroSerie" style="margin-bottom: 10px">21323121</p>
      </div>
    </div>
  </div>
  <div class="flexRow" style="justify-content: center; width: 100%">
    <table class="estiloTabla" id="exportable_table">
      <thead>
        <tr style="background-color: rgb(138, 138, 138); color: #fff">
          <th style="min-width: 20px; max-width: 20px; width: 20px">NÂº</th>
          <th style="min-width: 50px; max-width: 50px; width: 50px">ID</th>
          <th style="min-width: 200px; max-width: 200px; width: 200px">
            Producto
          </th>
          <th style="min-width: 130px; max-width: 130px; width: 130px">
            Presentacion
          </th>
          <th style="min-width: 130px; max-width: 130px; width: 130px">
            Valor Unit Venta
          </th>
          <th style="min-width: 130px; max-width: 130px; width: 130px">
            Cantidad
          </th>
          <th style="min-width: 130px; max-width: 130px; width: 130px">
            Tipo de Pago
          </th>
          <th style="min-width: 130px; max-width: 130px; width: 130px">
            Sub-Total
          </th>
        </tr>
      </thead>
      <tbody id="tablaSalidas" style="height: 60vh; max-height: 60vh"></tbody>
    </table>
  </div>
</div>
<div>
  <div>
    <div class="flexRow" style="justify-content: space-evenly">
      <div class="flexRow">
        <button
          style="margin-right: 10px"
          tabindex="6"
          id="btnImprimir"
          class="boton colorPrimario"
          onclick=""
        >
          GUARDAR COMO PDF
        </button>
        <button
          onclick="ExportExcel('xlsx')"
          tabindex="5"
          id="btnAceptar"
          class="boton colorSecundario"
        >
          EXPORTAR A EXCEL
        </button>
      </div>
    </div>
  </div>
</div>
<%- include('./partials/modal_nueva_aportacion.ejs') %>
<script src="scripts/html2pdf.bundle.min.js"></script>
<script src="scripts/documento_historial_salida.js"></script>
<%-include('./partials/footer.ejs') %>
